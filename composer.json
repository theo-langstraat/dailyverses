{
    "name": "theolangstraat/dailyverses",
    "description": "Show a daily verse from the bible of your choice",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Theo Langstraat",
            "email": "theo.langstraat@delta.nl"
        }
    ],
    "require": {
        "typo3/cms-core": "^13.4"
    },
    "autoload": {
        "psr-4": {
            "Theolangstraat\\Dailyverses\\": "Classes/"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "sort-packages": true
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "dailyverses"
        }
    }
}